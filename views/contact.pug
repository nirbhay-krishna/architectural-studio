extends base.pug

block scripts
    script(src='/static/index.js')

block style
    style
        include ../static/stylesheets/contact.css
        include ../static/stylesheets/style.css
block content
    div.box
        div.item1.auto Contact 
    div.box1
        form(action='/contact', method='post', class='myForm')
            input(type="text", class='myInput', name='name', placeholder='Name')
            input(type="email", class='myInput', name='email', placeholder='Email')
            input(type="phone", class='myInput', name='phone', placeholder='Phone')
            //- input(type="text", class='myInput', name='address', placeholder='enter address')
            input(type="text", class='myInput', name='desc', placeholder='Type your message here...')
            button(class='btn')
                | submit
